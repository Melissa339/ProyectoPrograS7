@model SistemaElecciones.Models.Partido

<form id="editar" asp-action="Editar" method="POST">
    <div class="row">
            <input hidden name="IdPartido" value="@Model.IdPartido" />
        <div class="form-group">
            <label class="control-label">Nombre</label>
            <input asp-for="Nombre" name="Nombre" id="Nombre" class="form-control" />
        </div>
        <div class="form-group">
            <label class="control-label">Fecha Fundación</label>
            <input asp-for="FechaFundacion" name="FechaFundacion" id="FechaFundacion" type="date" class="form-control" />
        </div>
        <div class="form-group">
            <label class="control-label">Sede</label>
            <input asp-for="Sede" name="Sede" id="Sede" class="form-control" />
        </div>
    </div>
</form>